<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <filter>
  	<filter-name>ExceptionFilter</filter-name>
  	<display-name>ExceptionFilter</display-name>
  	<description></description>
  	<filter-class>com.lames.admin.filter.ExceptionFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>ExceptionFilter</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
  	<servlet-name>ListToUpdate</servlet-name>
  	<display-name>ListToUpdate</display-name>
  	<description></description>
  	<servlet-class>com.lames.admin.servlet.merchantdetail.ListToUpdate</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateStatus</servlet-name>
  	<display-name>UpdateStatus</display-name>
  	<description></description>
  	<servlet-class>com.lames.admin.servlet.merchantdetail.UpdateStatus</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ListVerify</servlet-name>
  	<display-name>ListVerify</display-name>
  	<description></description>
  	<servlet-class>com.lames.admin.servlet.merchantdetail.ListVerify</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>VerifyStatus</servlet-name>
  	<display-name>VerifyStatus</display-name>
  	<description></description>
  	<servlet-class>com.lames.admin.servlet.merchantdetail.VerifyStatus</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetPassedShopID</servlet-name>
  	<display-name>GetPassedShopID</display-name>
  	<description></description>
  	<servlet-class>com.lames.admin.servlet.merchantdetail.GetPassedShopID</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FindMerchantDetail</servlet-name>
  	<display-name>FindMerchantDetail</display-name>
  	<description></description>
  	<servlet-class>com.lames.admin.servlet.merchantdetail.FindMerchantDetail</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MerchantLogin</servlet-name>
  	<display-name>MerchantLogin</display-name>
  	<description></description>
  	<servlet-class>com.lames.admin.servlet.merchant.MerchantLogin</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MerchantRegister</servlet-name>
  	<display-name>MerchantRegister</display-name>
  	<description></description>
  	<servlet-class>com.lames.admin.servlet.merchant.MerchantRegister</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TAdminServiceimpl</servlet-name>
  	<display-name>TAdminServiceimpl</display-name>
  	<description></description>
  	<servlet-class>com.lames.admin.service.impl.TAdminServiceimpl</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TAdminLogin</servlet-name>
  	<display-name>TAdminLogin</display-name>
  	<description></description>
  	<servlet-class>com.lames.admin.servlet.tadmin.TAdminLogin</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ListToUpdate</servlet-name>
  	<url-pattern>/MerchantDetail/ListToUpdate.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateStatus</servlet-name>
  	<url-pattern>/MerchantDetail/UpdateStatus.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ListVerify</servlet-name>
  	<url-pattern>/MerchantDetail/ListVerify.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>VerifyStatus</servlet-name>
  	<url-pattern>/MerchantDetail/VerifyStatus.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetPassedShopID</servlet-name>
  	<url-pattern>/GetPassedShopID.do</url-pattern><!-- C段获取商店ID列表不能过滤 -->
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FindMerchantDetail</servlet-name>
  	<url-pattern>/merchantdetail.do</url-pattern><!-- M端获取申请审核的信息不能过滤 -->
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MerchantLogin</servlet-name>
  	<url-pattern>/merchant/login.do</url-pattern><!-- 商家登陆不能过滤 -->
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MerchantRegister</servlet-name>
  	<url-pattern>/merchant/register.do</url-pattern><!-- 商家注册不能过滤 -->
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TAdminLogin</servlet-name>
  	<url-pattern>/MerchantDetail/Login.do</url-pattern><!-- 登陆不能过滤 -->
  </servlet-mapping>
</web-app>
